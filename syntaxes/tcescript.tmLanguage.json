{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "TCE Script",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#subKeywords"
		},
		{
			"include": "#constant"
		},
		{
			"include": "#strings"
		},
		{
			"name": "meta.selector",
			"match": "txt-[\\w-]+"
		},
		{
			"include": "#entity"
		},
		{
			"name": "keyword.other.unit",
			"match": "\\B(\\+[A-Za-z][A-Za-z_\\d-]+)"
		},
		{
			"name": "string.regexp",
			"match": "\\B(-[A-Za-z][A-Za-z_\\d-]+)"
		},
		{
			"name": "constant.numeric.tcescript",
			"match": "-?[\\d]*\\.\\.-?[\\d]*"
		},
		{
			"name": "constant.numeric.tcescript",
			"match": "\\b[\\d]+\\.?[\\d]*\\b"
		},
		{
			"name": "punctuation.definition.tag",
			"match": "[\\w\\/]+\\.png\\b"
		},
		{
			"name": "comment",
			"match": "#.*$"
		}
	],
